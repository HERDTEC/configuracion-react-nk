{
    "env": {
        "browser": true,
        "es6": true
    },

    "extends": ["standard", "react-app"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ]
}